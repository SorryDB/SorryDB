  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug morphcloud_runner",
      "type": "debugpy",
      "request": "launch",
      "module": "sorrydb.cli.run_morphcloud_runner",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": ["--sorry-file", "data/mock_sorry.json",
    "--output-dir", "outputs/debug"]
    },
    {
      "name": "Debug run_agentic",
      "type": "debugpy",
      "request": "launch",
      "module": "sorrydb.cli.run_agentic",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug run_supersimple",
      "type": "debugpy",
      "request": "launch",
      "module": "sorrydb.cli.run_supersimple",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug run_rfl_agent",
      "type": "debugpy",
      "request": "launch",
      "module": "sorrydb.cli.run_rfl_agent",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": [
        "--sorry-file",
        "doc/sample_sorry_list.json",
        "--output-file",
        "outputs/simple/result.json",
        "--lean-data",
        "${workspaceFolder}/.lean_folder",
        "--log-file",
        "output.log"
      ]
    },
    {
      "name": "Debug morphcloud_local",
      "type": "debugpy",
      "request": "launch",
      "module": "sorrydb.cli.run_morphcloud_local",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": [
        "--repo-path",
        "tests/mock_lean_repository",
        "--sorry-path",
        "tests/mock_sorries/single_sorry.json",
        "--agent-strategy",
        "{\"name\":\"rfl\"}",
        "--output-path",
        "outputs/local"
      ]
    }
  ]
}
