# LeaderboardRunner Authentication Configuration
# Copy this file to .env and fill in your credentials

# Required: Your leaderboard username (email)
LEADERBOARD_USERNAME=your-email@example.com

# Required: Your leaderboard password
LEADERBOARD_PASSWORD=your-password

# Optional: Leaderboard API host (defaults to http://127.0.0.1:8000)
LEADERBOARD_HOST=http://127.0.0.1:8000

# Optional: Initial admin credentials for API server setup
INITIAL_ADMIN_EMAIL=admin@example.com
INITIAL_ADMIN_PASSWORD=admin-password
